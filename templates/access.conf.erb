# allow only the groups listed
<% if scope.lookupvar('pam::allowed_users').class == String -%>
+ : <%= scope.lookupvar('pam::allowed_users') %> : ALL
<% else -%>
<% scope.lookupvar('pam::allowed_users').each do |user| -%>
+ : <%= user %> : ALL
<% end -%>
<% end -%>

# default deny
- : ALL : ALL
